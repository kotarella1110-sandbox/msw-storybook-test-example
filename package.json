{
  "name": "msw-storybook-testing-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "run-s check:*",
    "check:tsc": "tsc --noEmit",
    "check:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --max-warnings=0",
    "check:prettier": "prettier . --ignore-path .gitignore --check",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix",
    "fix:prettier": "prettier . --ignore-path .gitignore --write --cache"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-styled-components-a11y": "^1.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
