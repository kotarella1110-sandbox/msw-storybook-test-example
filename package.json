{
  "name": "msw-storybook-testing-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "run-s check:*",
    "check:tsc": "tsc --noEmit",
    "check:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --max-warnings=0",
    "check:prettier": "prettier . --ignore-path .gitignore --check",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix",
    "fix:prettier": "prettier . --ignore-path .gitignore --write --cache",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-styled-components-a11y": "^1.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
